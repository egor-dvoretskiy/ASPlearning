@using Tutorial2.Domain.Entity
@model List<Car>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@foreach (var car in Model)
{
    <p>@car.Name</p>
    <p>@car.Model</p>
    <p>@car.Description</p>
    <p>@car.Price</p>
    <p>@car.DateCreate</p>
    <p>@car.Speed</p>
}
